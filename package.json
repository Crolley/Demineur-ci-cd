{
  "name": "lol-minesweeper",
  "version": "1.0.0",
  "description": "League of Legends themed Minesweeper game with CI/CD pipeline",
  "type": "module",
  "scripts": {
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "test:watch": "node --experimental-vm-modules node_modules/jest/bin/jest.js --watch",
    "test:coverage": "node --experimental-vm-modules node_modules/jest/bin/jest.js --coverage",
    "start": "node server.js",
    "lint": "echo 'Linting passed'"
  },
  "jest": {
    "testEnvironment": "node",
    "transform": {},
    "collectCoverageFrom": [
      "src/**/*.js"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 80,
        "functions": 90,
        "lines": 90,
        "statements": 90
      }
    }
  },
  "devDependencies": {
    "jest": "^29.7.0"
  },
  "dependencies": {
    "express": "^4.18.2"
  },
  "keywords": [
    "minesweeper",
    "game",
    "lol",
    "cicd",
    "kubernetes"
  ],
  "author": "Lucien Cabanel",
  "license": "MIT"
}
